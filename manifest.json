{
  "name": "Canvas improver",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs",
        "notifications"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
        "https://*/"
    ]
}